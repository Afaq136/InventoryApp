{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@firebaseConfig": [
        "FirebaseConfig.ts"
      ],
      "@itemsService": [
        "services/itemService.ts"
      ],
      "@darkModeContext" : [
        "app/context/DarkModeContext.tsx"
      ],
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.tsx",
    "nativewind-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
